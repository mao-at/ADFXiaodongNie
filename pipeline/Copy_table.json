{
	"name": "Copy_table",
	"properties": {
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "sql_dataset",
						"type": "DatasetReference",
						"parameters": {
							"schema": {
								"value": "@pipeline().parameters.sourceSchema",
								"type": "Expression"
							},
							"tableName": {
								"value": "@pipeline().parameters.sourceTablename",
								"type": "Expression"
							},
							"dbName": {
								"value": "@pipeline().parameters.sourceDBname",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "sql_dataset",
						"type": "DatasetReference",
						"parameters": {
							"schema": {
								"value": "@pipeline().parameters.sinkSchema",
								"type": "Expression"
							},
							"tableName": {
								"value": "@pipeline().parameters.sinkTablename",
								"type": "Expression"
							},
							"dbName": {
								"value": "@pipeline().parameters.sinkDBname",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"sourceSchema": {
				"type": "String"
			},
			"sourceTablename": {
				"type": "String"
			},
			"sourceDBname": {
				"type": "String"
			},
			"sinkSchema": {
				"type": "String"
			},
			"sinkTablename": {
				"type": "String"
			},
			"sinkDBname": {
				"type": "String"
			}
		},
		"annotations": []
	}
}