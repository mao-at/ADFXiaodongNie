{
	"name": "flowlet1",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sql_dataset",
						"type": "DatasetReference"
					},
					"name": "ODScustomers"
				},
				{
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "AddHashID",
					"description": "Add hash value column."
				}
			],
			"scriptLines": [
				"source(output(",
				"          CustomerID as integer,",
				"          CustomerName as string,",
				"          BillToCustomerID as integer,",
				"          CustomerCategoryID as integer,",
				"          BuyingGroupID as integer,",
				"          PrimaryContactPersonID as integer,",
				"          PostalCityID as integer,",
				"          ValidFrom as timestamp,",
				"          ValidTo as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> ODScustomers",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"ODScustomers derive(HashId = md5(CustomerID,CustomerName)) ~> AddHashID"
			]
		}
	}
}